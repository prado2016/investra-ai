# Local Development Environment Variables (not committed to version control)
# Copy this file to .env.local and fill in your actual values

# Yahoo Finance API Configuration (optional - Yahoo Finance doesn't require API key for basic functionality)
VITE_YAHOO_FINANCE_API_KEY=

# Application Configuration
VITE_APP_ENVIRONMENT=development
VITE_DEBUG_MODE=true

# Cache Configuration (optional - defaults will be used if not specified)
VITE_CACHE_DURATION=60000
VITE_HISTORICAL_CACHE_DURATION=600000
